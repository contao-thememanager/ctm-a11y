{
  "name": "contao-thememanager/ctm-accessibility",
  "type": "symfony-bundle",
  "description": "A11y enhancement for CTM",
  "homepage": "https://www.contao-thememanager.com/",
  "authors": [
    {
      "name": "Sebastian Zoglowek",
      "homepage": "https://github.com/zoglo",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.2",
    "contao-thememanager/core": "^2.1"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "ContaoThemeManager\\Accessibility\\": "src/"
    },
    "classmap": [
      "contao/"
    ],
    "exclude-from-classmap": [
      "contao/templates/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "contao-manager-plugin": "ContaoThemeManager\\Accessibility\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "contao/manager-plugin": true
    }
  }
}
